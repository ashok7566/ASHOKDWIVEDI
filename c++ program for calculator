using namespace std;
#include<iostream>
int main()
{
char ch;
int a,b;
cout<<"enter the value for a and b";
cin>>a>>b;
cout<<"+"<<endl;
cout<<"_"<<endl;
cout<<"*"<<endl;
cout<<"/"<<endl;
cout<<"enter the symbol to perform operation"<<endl;
cin>>ch;
switch(ch)
{
case '+':
          cout<<a+b;
          break;
case '_' :
          cout<<a-b;
          break;
case '*':
          cout<<a*b;
          break;
case '/':
          cout<<a/b;
          break;
default : cout<<"invallid choice";
          break;
}
return 0;
}
